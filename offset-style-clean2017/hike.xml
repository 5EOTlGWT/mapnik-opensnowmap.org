<Style name="snowshoeing">

	<!--no-difficulty-->
	<Rule>
		&maxscale_zoom9;
		&minscale_zoom12;
		<ElseFilter/>
		<LineSymbolizer stroke-width="1" stroke="#FF9FAF" stroke-gamma=".6"/>
	</Rule>
	<Rule>
		&maxscale_zoom13;
		&minscale_zoom18;
		<ElseFilter/>
		<LineSymbolizer stroke-width="2.5" stroke="#FF9FAF" 
		stroke-linejoin="round" stroke-gamma=".6"/>
	</Rule>
	<Rule>
		&maxscale_zoom13;
		&minscale_zoom18;
		<ElseFilter/>
		<LineSymbolizer stroke-width="6" stroke="#FF9FAF" 
		stroke-dasharray="1,10"
		stroke-linejoin="round" stroke-linecap="round" stroke-gamma=".6"/>
	</Rule>
	<Rule>
		&maxscale_zoom13;
		&minscale_zoom18;
		<ElseFilter/>
		<LineSymbolizer stroke-width="5" stroke="#FFFFFF" 
		stroke-dasharray="1,10"
		stroke-linejoin="round" stroke-linecap="round" stroke-gamma=".6"/>
	</Rule>
</Style>
