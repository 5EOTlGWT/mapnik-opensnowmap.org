
<Style name="site_text" >

     <Rule>
      &maxscale_zoom11;
      &minscale_zoom12;

      <TextSymbolizer fontset-name="book-fonts"
       size="11"
       fill="#003074" halo-fill= "rgba(255,255,255,0.6)" halo-radius="1.5"
       spacing="1000" 
      horizontal-alignment="middle"
      vertical-alignment="middle"
      
      allow-overlap="false" avoid-edges="true"
      >[site_name]</TextSymbolizer>

    </Rule>
     <Rule>
      <!--  Fill the availble placement with spaces  -->
      &maxscale_zoom10;
      &minscale_zoom10;

      <TextSymbolizer fontset-name="book-fonts"
       size="11"
       fill="#003074" halo-fill= "rgba(255,255,255,0.6)" halo-radius="1.5"
       spacing="1000" 
      horizontal-alignment="middle"
      vertical-alignment="middle"
      
      allow-overlap="false" avoid-edges="true"
      >"       "</TextSymbolizer>

    </Rule>
    
