<Style name="others">
	<Rule>
		&maxscale_zoom9;
		&minscale_zoom18;
		<Filter>[piste:type]='snow_park' </Filter>
		<LineSymbolizer stroke-width="2.5" stroke="#FFFFFF" smooth="0.5" stroke-gamma=".6"/>
	</Rule>
	<Rule>
		&maxscale_zoom9;
		&minscale_zoom18;
		<Filter>[piste:type]='snow_park' </Filter>
		<LineSymbolizer stroke-width="2" stroke="#0F6DD3" smooth="0.5" stroke-gamma=".6"/>
	</Rule>
	
	<Rule>
		&maxscale_zoom9;
		&minscale_zoom18;
		<Filter>[piste:type]='playground' </Filter>
		<LineSymbolizer stroke-width="2.5" stroke="#FFFFFF" smooth="0.5" stroke-gamma=".6"/>
	</Rule>
	<Rule>
		&maxscale_zoom9;
		&minscale_zoom18;
		<Filter>[piste:type]='playground' </Filter>
		<LineSymbolizer stroke-width="2" stroke="#A6E05B" smooth="0.5" stroke-gamma=".6"/>
	</Rule>
	
	<Rule>
		&maxscale_zoom9;
		&minscale_zoom18;
		<Filter>[piste:type]='sled' </Filter>
		<LineSymbolizer stroke-width="3" stroke="#FFFFFF" smooth="0.5" stroke-gamma=".6"/>
	</Rule>
	<Rule>
		&maxscale_zoom9;
		&minscale_zoom18;
		<Filter>[piste:type]='sled' </Filter>
		<LineSymbolizer stroke-width="1.5" stroke="#BAFF3A" smooth="0.5" stroke-gamma=".6"/>
	</Rule>
	
	<Rule>
		&maxscale_zoom9;
		&minscale_zoom18;
		<Filter>[piste:type]='ice_skate' or [sport]='skating'</Filter>
		<LineSymbolizer stroke-width="3" stroke="#FFFFFF" smooth="0.5" stroke-gamma=".6"/>
	</Rule>
	<Rule>
		&maxscale_zoom9;
		&minscale_zoom18;
		<Filter>[piste:type]='ice_skate' or [sport]='skating'</Filter>
		<LineSymbolizer stroke-width="1.5" stroke="#6FC4D0" smooth="0.5" stroke-gamma=".6"/>
	</Rule>
	
	<Rule>
		&maxscale_zoom9;
		&minscale_zoom18;
		<Filter>[piste:type]='sleigh'</Filter>
		<LineSymbolizer stroke-width="3" stroke="#FFFFFF" smooth="0.5" stroke-gamma=".6"/>
	</Rule>
	<Rule>
		&maxscale_zoom9;
		&minscale_zoom18;
		<Filter>[piste:type]='sleigh'</Filter>
		<LineSymbolizer stroke-width="1.5" stroke="#8F54D8" smooth="0.5" stroke-gamma=".6"/>
	</Rule>
	
</Style>
<Style name="others-icons">
	<Rule>
		&maxscale_zoom15;
		&minscale_zoom18;
		<Filter>[piste:type]='snow_park'</Filter>
		<PointSymbolizer file="pics/snow_park_bw.png" />
	</Rule>
	<Rule>
		&maxscale_zoom15;
		&minscale_zoom18;
		<Filter>[piste:type]='sled' </Filter>
		<ShieldSymbolizer file="pics/sled_bw.png" fontset-name="bold-fonts"
		spacing="5000" allow-overlap="false"
		placement="point">' '</ShieldSymbolizer>
	</Rule>
	<Rule>
		&maxscale_zoom15;
		&minscale_zoom18;
		<Filter>[piste:type]='ice_skate' or [sport]='skating'</Filter>
		<MarkersSymbolizer file="pics/ice-skate.png"
		placement="point" spacing="400"/>
	</Rule>
	<Rule>
		&maxscale_zoom15;
		&minscale_zoom18;
		<Filter>[piste:type]='sleigh' </Filter>
		<ShieldSymbolizer file="pics/sleigh_bw.png" fontset-name="bold-fonts"
		min-distance="5000" allow-overlap="false"
		placement="point">' '</ShieldSymbolizer>
	</Rule>
	<Rule>
		&maxscale_zoom15;
		&minscale_zoom18;
		<Filter>[piste:type]='playground' </Filter>
		<PointSymbolizer file="pics/playground_bw.png"/>
	</Rule>

	<Rule>
		&maxscale_zoom15;
		&minscale_zoom18;
		<Filter>[piste:grooming]='mogul'</Filter>
		<PointSymbolizer file="pics/mogul.svg" transform="scale(0.6,0.6)"/>
	</Rule>
	<Rule>
		&maxscale_zoom15;
		&minscale_zoom18;
		<Filter>[gladed]='yes' </Filter>
		<PointSymbolizer file="pics/gladed.svg" transform="scale(0.6,0.6)"/>
	</Rule>
	<Rule>
		&maxscale_zoom15;
		&minscale_zoom18;
		<Filter>[lit]='yes' or [piste:lit]='yes'</Filter>
		<PointSymbolizer file="pics/light.png"/>
	</Rule>
</Style>
<Style name="others-area">
	<!--novice-->
	<Rule>
		&maxscale_zoom9;
		&minscale_zoom18;
		<Filter>[piste:type]='snow_park'</Filter>
		<PolygonSymbolizer fill="#0074F3" fill-opacity="0.5" smooth="0.5" gamma=".6"/>
	</Rule>
	<Rule>
		&maxscale_zoom9;
		&minscale_zoom18;
		<Filter>[piste:type]='playground'</Filter>
		<PolygonSymbolizer fill="#A6E05B" fill-opacity="0.5" smooth="0.5" gamma=".6"/>
	</Rule>
</Style>
<Style name="other-text"> 
	<Rule>
		&maxscale_zoom15;
		&minscale_zoom18;
		<Filter>[piste:type]='snow_park' </Filter>
		<TextSymbolizer fontset-name="book-fonts"
		size="10" fill="#005CC0" halo-fill= "#DDDDDD" halo-radius="1" 
		placement="point" allow-overlap="false" spacing="10000" 
		avoid-edges="true" dy="15"
		wrap-width="15" >[name]</TextSymbolizer>
	</Rule>
	<Rule>
		&maxscale_zoom15;
		&minscale_zoom18;
		<Filter>[piste:type]='playground' </Filter>
		<TextSymbolizer fontset-name="book-fonts"
		size="10" fill="#6CAD1A" halo-fill= "#DDDDDD" halo-radius="1" 
		placement="point" allow-overlap="false" spacing="10000" 
		avoid-edges="true" dy="15"
		wrap-width="15" >[name]</TextSymbolizer>
	</Rule>
	<Rule>
		&maxscale_zoom15;
		&minscale_zoom18;
		<Filter>[piste:type]='sled' </Filter>
		<TextSymbolizer fontset-name="book-fonts"
		size="10" fill="#70A808" halo-fill= "#DDDDDD" halo-radius="1" 
		placement="point" allow-overlap="false" spacing="1000" 
		avoid-edges="true" dy="15"
		wrap-width="15" >[name]</TextSymbolizer>
	</Rule>

	<Rule>
		&maxscale_zoom15;
		&minscale_zoom18;
		<Filter>[piste:type]='ski_jump' or [piste:type]='jump' or [sport]='ski_jump' or [sport]='ski_jump_take_off'</Filter>
		<TextSymbolizer fontset-name="book-fonts"
		size="10" fill="#655ABE" halo-fill= "#DDDDDD" halo-radius="1" 
		placement="point" allow-overlap="false" spacing="10000" 
		avoid-edges="true" dx='-30'
		wrap-width="15" >[name]</TextSymbolizer>
	</Rule>
</Style>
<Style name="ski_jump-text"> 
	<Rule>
		&maxscale_zoom15;
		&minscale_zoom18;
		<Filter>not [takeoff]=''</Filter>
		<TextSymbolizer fontset-name="book-fonts"
		size="8" fill="#655ABE" halo-fill= "#DDDDDD" halo-radius="2" 
		placement="point" allow-overlap="false" spacing="10000" 
		avoid-edges="true" dy="10"
		wrap-width="15" >[takeoff]</TextSymbolizer>
	</Rule>
</Style>
<Style name="ski_jump-area">

	<Rule>
		&maxscale_zoom12;
		&minscale_zoom18;
		<Filter>[piste:type]='ski_jump_landing'</Filter>
		<PolygonSymbolizer fill="#BCB7E1" fill-opacity="0.7" smooth="0.5" gamma=".6"/>
	</Rule>
</Style>
<Style name="ski_jump">
	
	<Rule>
		&maxscale_zoom12;
		&minscale_zoom18;
		<Filter>[piste:type]='ski_jump_landing' and [area]='yes'</Filter>
		<PolygonSymbolizer fill="#BCB7E1" fill-opacity="0.5" smooth="0.5" gamma=".6"/>
	</Rule>
	<Rule>
		&maxscale_zoom9;
		&minscale_zoom18;
		<Filter>[piste:type]='ski_jump' or [piste:type]='jump' or [piste:type]='ski_jump_landing' or [sport]='ski_jump_take_off' or [sport]='ski_jump' and not [area]='yes'</Filter>
		<LineSymbolizer stroke-width="2.5" stroke="#E7E3FF" smooth="0.5" stroke-gamma=".6"/>
	</Rule>
	<Rule>
		&maxscale_zoom9;
		&minscale_zoom18;
		<Filter>[piste:type]='ski_jump' or [piste:type]='jump' or [sport]='ski_jump' or [sport]='ski_jump_take_off'</Filter>
		<LineSymbolizer stroke-width="2" stroke="#655ABE" smooth="0.5" stroke-gamma=".6"/>
	</Rule>
	<Rule>
		&maxscale_zoom12;
		&minscale_zoom18;
		<Filter>[piste:type]='ski_jump_landing' and not [area]='yes'</Filter>
		<LineSymbolizer stroke-width="2" stroke="#655ABE" smooth="0.5" stroke-gamma=".6"
		stroke-dasharray="6,6"/>
	</Rule>
	<Rule>
		&maxscale_zoom12;
		&minscale_zoom18;
		<Filter>[piste:type]='ski_jump_landing' and [area]='yes'</Filter>
		<LineSymbolizer stroke-width="1" stroke="#655ABE" smooth="0.5" stroke-gamma=".6"
		stroke-dasharray="6,6"/>
	</Rule>
	
	<Rule>
		&maxscale_zoom15;
		&minscale_zoom18;
		<Filter>[piste:type]='ski_jump' or [sport]='ski_jump' or [sport]='ski_jump_take_off'</Filter>
		<ShieldSymbolizer file="pics/jump_bw.png" fontset-name="bold-fonts"
		spacing="10000" allow-overlap="false"
		placement="point">' '</ShieldSymbolizer>
	</Rule>
</Style>

<Style name="POIs">
	<Rule>
		&maxscale_zoom16;
		&minscale_zoom18;
		<Filter>[man_made]='snow_cannon'</Filter>
		<PointSymbolizer file="pics/snow_cannon.png"/>
	</Rule>
</Style>
