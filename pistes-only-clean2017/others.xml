<Style name="others-icons">
	<Rule>
		&maxscale_zoom15;
		&minscale_zoom18;
		<Filter>[piste:type].match('.*snow_park.*')</Filter>

        <TextSymbolizer fontset-name="ski-fonts"
       size="20" opacity="1" fill="#000000" halo-fill= "rgba(0,116,243,0.3)"  halo-radius="4"
      horizontal-alignment="middle" vertical-alignment="middle"
      placement="point" label-position-tolerance="40"
      allow-overlap="false" avoid-edges="true"
      >i
      </TextSymbolizer>
	</Rule>
	<Rule>
		&maxscale_zoom15;
		&minscale_zoom18;
		<Filter>[piste:type].match('.*sled.*')</Filter>
        <TextSymbolizer fontset-name="ski-fonts"
       size="18" opacity="1" fill="#000000" halo-fill= "rgba(186,255,58,0.6)"  halo-radius="4"
      horizontal-alignment="middle" vertical-alignment="middle"
      placement="point" label-position-tolerance="40"
      allow-overlap="false" avoid-edges="true"
      >g
      </TextSymbolizer>
	</Rule>
	<Rule>
		&maxscale_zoom15;
		&minscale_zoom18;
		<Filter>[piste:type].match('.*skat.*') or [sport].match('.*skat.*')</Filter>
        <TextSymbolizer fontset-name="ski-fonts"
       size="18" opacity="1" fill="#1A9CCC" halo-fill= "rgba(255,255,255,0.6)"  halo-radius="4"
      horizontal-alignment="middle" vertical-alignment="middle"
      placement="point" label-position-tolerance="40"
      allow-overlap="false" avoid-edges="true"
      >d
      </TextSymbolizer>
<!--
		<MarkersSymbolizer file="pics/ice-skate.png"
		placement="point" spacing="400"/>
-->
	</Rule>
	<Rule>
		&maxscale_zoom15;
		&minscale_zoom18;
		<Filter>[sport].match('.*curling.*')</Filter>
        <TextSymbolizer fontset-name="ski-fonts"
       size="16" opacity="1" fill="#1A9CCC" halo-fill= "rgba(255,255,255,0.6)"  halo-radius="4"
      horizontal-alignment="middle" vertical-alignment="middle"
      placement="point" label-position-tolerance="40"
      allow-overlap="false" avoid-edges="true"
      >l
      </TextSymbolizer>
	</Rule>
	<Rule>
		&maxscale_zoom15;
		&minscale_zoom18;
		<Filter>[sport].match('.*ice_stock.*')</Filter>

        <TextSymbolizer fontset-name="ski-fonts"
       size="16" opacity="1" fill="#1A9CCC" halo-fill= "rgba(255,255,255,0.6)"  halo-radius="4"
      horizontal-alignment="middle" vertical-alignment="middle"
      placement="point" label-position-tolerance="40"
      allow-overlap="false" avoid-edges="true"
      >m
      </TextSymbolizer>
	</Rule>
	<Rule>
		&maxscale_zoom15;
		&minscale_zoom18;
		<Filter>[sport].match('.*hockey.*')</Filter>

        <TextSymbolizer fontset-name="ski-fonts"
       size="18" opacity="1" fill="#1A9CCC" halo-fill= "rgba(255,255,255,0.6)"  halo-radius="4"
      horizontal-alignment="middle" vertical-alignment="middle"
      placement="point" label-position-tolerance="40"
      allow-overlap="false" avoid-edges="true"
      >k
      </TextSymbolizer>
	</Rule>
	<Rule>
		&maxscale_zoom15;
		&minscale_zoom18;
		<Filter>[piste:type].match('.*sleigh.*')</Filter>
      <TextSymbolizer fontset-name="ski-fonts"
       size="20" opacity="1" fill="#8F54D8" 
       halo-fill= "rgba(255,255,255,0.5)" halo-radius="2"
       placement="point"  label-position-tolerance="40"
       repeat-distance="200"
       allow-overlap="false" avoid-edges="true">
       h
      </TextSymbolizer>
	</Rule>
	<Rule>
		&maxscale_zoom15;
		&minscale_zoom18;
		<Filter>[piste:type].match('.*playground.*') </Filter>
      <TextSymbolizer fontset-name="ski-fonts"
       size="20" opacity="1" fill="#000000" 
       halo-fill= "rgba(166,224,91,0.3)" halo-radius="2"
       placement="point"  label-position-tolerance="40"
       repeat-distance="200"
       allow-overlap="false" avoid-edges="true">
       c
      </TextSymbolizer>
	</Rule>

	<Rule>
		&maxscale_zoom15;
		&minscale_zoom18;
		<Filter>[piste:grooming]='mogul'</Filter>
		<PointSymbolizer file="pics/mogul.svg" transform="scale(0.6,0.6)"/>
	</Rule>
	<Rule>
		&maxscale_zoom15;
		&minscale_zoom18;
		<Filter>[gladed]='yes' </Filter>
		<PointSymbolizer file="pics/gladed.svg" transform="scale(0.6,0.6)"/>
	</Rule>
	<Rule>
		&maxscale_zoom15;
		&minscale_zoom18;
		<Filter>[lit]='yes' or [piste:lit]='yes'</Filter>
		<PointSymbolizer file="pics/light.png"/>
	</Rule>
</Style>
<Style name="others-area">
	<!--novice-->
	<Rule>
		&maxscale_zoom9;
		&minscale_zoom18;
		<Filter>[piste:type].match('.*snow_park.*')</Filter>
		<PolygonSymbolizer fill="#0074F3" fill-opacity="0.5" smooth="0.5" gamma=".6"/>
	</Rule>
	<Rule>
		&maxscale_zoom9;
		&minscale_zoom18;
		<Filter>[piste:type].match('.*playground.*')</Filter>
		<PolygonSymbolizer fill="#A6E05B" fill-opacity="0.5" smooth="0.5" gamma=".6"/>
	</Rule>
</Style>
<Style name="other-text"> 
	<Rule>
		&maxscale_zoom15;
		&minscale_zoom18;
		<Filter>[piste:type].match('.*snow_park.*')</Filter>
		<TextSymbolizer fontset-name="book-fonts"
		size="10" fill="#005CC0" halo-fill= "#DDDDDD" halo-radius="1" 
		placement="point" allow-overlap="false" spacing="10000" 
		avoid-edges="true" dy="15"
		wrap-width="25" >[name]</TextSymbolizer>
	</Rule>
	<Rule>
		&maxscale_zoom15;
		&minscale_zoom18;
		<Filter>[piste:type].match('.*playground.*')</Filter>
		<TextSymbolizer fontset-name="book-fonts"
		size="10" fill="#6CAD1A" halo-fill= "#DDDDDD" halo-radius="1" 
		placement="point" allow-overlap="false" spacing="10000" 
		avoid-edges="true" dy="15"
		wrap-width="25" >[name]</TextSymbolizer>
	</Rule>
	<Rule>
		&maxscale_zoom15;
		&minscale_zoom18;
		<Filter>[piste:type].match('.*sled.*')</Filter>
		<TextSymbolizer fontset-name="book-fonts"
		size="10" fill="#568400" halo-fill= "#FFFFFF" halo-radius="1" 
		placement="point" allow-overlap="false" spacing="1000" 
		avoid-edges="true" dy="15"
		wrap-width="25" >[name]</TextSymbolizer>
	</Rule>

	<Rule>
		&maxscale_zoom15;
		&minscale_zoom18;
		<Filter>[piste:type].match('.*ski_jump.*') or [piste:type].match('.*jump.*') or [sport].match('.*ski_jump.*') or [sport].match('.*ski_jump_take_off.*')</Filter>
		<TextSymbolizer fontset-name="book-fonts"
		size="10" fill="#655ABE" halo-fill= "#DDDDDD" halo-radius="1" 
		placement="point" allow-overlap="false" spacing="10000" 
		avoid-edges="true" dx='-30'
		wrap-width="25" >[name]</TextSymbolizer>
	</Rule>
</Style>
<Style name="ski_jump-text"> 
	<Rule>
		&maxscale_zoom15;
		&minscale_zoom18;
		<Filter>not [takeoff]=''</Filter>
		<TextSymbolizer fontset-name="book-fonts"
		size="8" fill="#655ABE" halo-fill= "#DDDDDD" halo-radius="2" 
		placement="point" allow-overlap="false" spacing="10000" 
		avoid-edges="true" dy="10"
		wrap-width="25" >[takeoff]</TextSymbolizer>
	</Rule>
</Style>
<Style name="ski_jump_area">

	<Rule>
		&maxscale_zoom12;
		&minscale_zoom18;
		<Filter>[piste:type].match('.*ski_jump_landing.*')</Filter>
		<PolygonSymbolizer fill="#BCB7E1" fill-opacity="0.7" smooth="0.5" gamma=".6"/>
	</Rule>
</Style>
<Style name="ski_jump">
	
	<Rule>
		&maxscale_zoom12;
		&minscale_zoom18;
		<Filter>[piste:type].match('.*ski_jump_landing.*') and [area]='yes'</Filter>
		<PolygonSymbolizer fill="#BCB7E1" fill-opacity="0.5" smooth="0.5" gamma=".6"/>
	</Rule>
	<Rule>
		&maxscale_zoom9;
		&minscale_zoom18;
		<Filter>([piste:type].match('.*ski_jump.*') or [piste:type].match('.*jump.*') or [sport].match('.*ski_jump.*') or [sport].match('.*ski_jump_take_off.*')) and not [area]='yes'</Filter>
		<LineSymbolizer stroke-width="2.5" stroke="#E7E3FF" smooth="0.5" stroke-gamma=".6"/>
	</Rule>
	<Rule>
		&maxscale_zoom9;
		&minscale_zoom18;
		<Filter>[piste:type].match('.*ski_jump.*') or [piste:type].match('.*jump.*') or [sport].match('.*ski_jump.*') or [sport].match('.*ski_jump_take_off.*')</Filter>
		<LineSymbolizer stroke-width="2" stroke="#655ABE" smooth="0.5" stroke-gamma=".6"/>
	</Rule>
	<Rule>
		&maxscale_zoom12;
		&minscale_zoom18;
		<Filter>[piste:type].match('.*ski_jump_landing.*') and not [area]='yes'</Filter>
		<LineSymbolizer stroke-width="2" stroke="#655ABE" smooth="0.5" stroke-gamma=".6"
		stroke-dasharray="6,6"/>
	</Rule>
	<Rule>
		&maxscale_zoom12;
		&minscale_zoom18;
		<Filter>[piste:type].match('.*ski_jump_landing.*') and [area]='yes'</Filter>
		<LineSymbolizer stroke-width="1" stroke="#655ABE" smooth="0.5" stroke-gamma=".6"
		stroke-dasharray="6,6"/>
	</Rule>
	
	<Rule>
		&maxscale_zoom15;
		&minscale_zoom18;
		<Filter>[piste:type].match('.*ski_jump.*') or [piste:type].match('.*jump.*') or [sport].match('.*ski_jump.*') or [sport].match('.*ski_jump_take_off.*')</Filter>
		<ShieldSymbolizer file="pics/jump_bw.png" fontset-name="bold-fonts"
		spacing="10000" allow-overlap="false"
		placement="point">' '</ShieldSymbolizer>
	</Rule>
</Style>

<Style name="POIs">
	<Rule>
		&maxscale_zoom16;
		&minscale_zoom18;
		<Filter>[man_made]='snow_cannon'</Filter>
		<PointSymbolizer file="pics/snow_cannon.png"/>
	</Rule>
</Style>
