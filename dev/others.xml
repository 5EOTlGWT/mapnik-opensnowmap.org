<Style name="others">
	<Rule>
		&maxscale_zoom9;
		&minscale_zoom18;
		<Filter>[piste:type]='snow_park' </Filter>
		<LineSymbolizer stroke-width="2" stroke="#0F6DD3"/>
	</Rule>
	
	<Rule>
		&maxscale_zoom9;
		&minscale_zoom18;
		<Filter>[piste:type]='playground' </Filter>
		<LineSymbolizer stroke-width="2" stroke="#A6E05B"/>
	</Rule>
	<Rule>
		&maxscale_zoom9;
		&minscale_zoom18;
		<Filter>[piste:type]='ski_jump' or [sport]='ski_jump' or [sport]='ski_jump_take_off'</Filter>
		<LineSymbolizer stroke-width="2" stroke="#655ABE"/>
	</Rule>
	
	<Rule>
		&maxscale_zoom9;
		&minscale_zoom18;
		<Filter>[piste:type]='sled' </Filter>
		<LineSymbolizer stroke-width="2" stroke="#BAFF3A"/>
	</Rule>
	
	<Rule>
		&maxscale_zoom9;
		&minscale_zoom18;
		<Filter>[piste:type]='ice_skate' or [sport]='skating'</Filter>
		<LineSymbolizer stroke-width="2" stroke="#6FC4D0"/>
	</Rule>
	
</Style>
<Style name="others-area">
	<!--novice-->
	<Rule>
		&maxscale_zoom9;
		&minscale_zoom18;
		<Filter>[piste:type]='snow_park'</Filter>
		<PolygonSymbolizer fill="#0074F3" fill-opacity="0.5" />
	</Rule>

	<Rule>
		&maxscale_zoom9;
		&minscale_zoom18;
		<Filter>[piste:type]='playground'</Filter>
		<PolygonSymbolizer fill="#A6E05B" fill-opacity="0.5" />
	</Rule>
</Style>
